{
	"name": "accounts",
	"version": "1.0.0",
	"description": "An Application for accounting",
	"main": "index.js",
	"dependencies": {
		"@prisma/client": "^2.19.0",
		"apollo-server-express": "^2.22.1",
		"bcryptjs": "^2.4.3",
		"body-parser": "^1.19.0",
		"cors": "^2.8.5",
		"deep-email-validator": "^0.1.18",
		"express": "^4.17.1",
		"graphql": "^15.5.0",
		"joi": "^17.4.0",
		"jsonwebtoken": "^8.5.1",
		"moment": "^2.29.1",
		"morgan": "^1.10.0"
	},
	"devDependencies": {
		"cross-env": "^7.0.3",
		"depcheck": "^1.4.0",
		"dotenv": "^8.2.0",
		"esm": "^3.2.25",
		"nodemon": "^2.0.7",
		"prisma": "^2.19.0"
	},
	"scripts": {
		"dev": "cross-env NODE_ENV=development nodemon -r esm .",
		"start": "cross-env NODE_ENV=production node -r esm .",
		"up": "docker-compose up -d",
		"postup": "npm run prisma:deploy; npm run dev",
		"down": "docker-compose down",
		"down:hard": "docker-compose down -v",
		"stop": "docker-compose stop",
		"deep-check": "depcheck --ignores esm",
		"prisma:format": "prisma format",
		"preprisma:deploy": "npm run prisma:format",
		"prisma:deploy": "prisma migrate dev --name dev",
		"prisma:seed": "prisma db seed --preview-feature"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/shahanahmed86/accounts.git"
	},
	"keywords": [
		"Accounts",
		"Data Entry"
	],
	"author": "Shahan Ahmed Khan",
